{
  "name": "eigen_service",
  "version": "1.0.0",
  "main": "src/app.ts",
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn build && node ./build/src/app.js",
    "build": "yarn && ./node_modules/.bin/tsc",
    "test": "yarn build && mocha ./build/**/*.test.js",
    "clean": "rm -f ./data/db_*.sqlite",
    "build_image": "docker build -t ieigen/service:v1 .",
    "deploy": "yarn build_image && docker run --name=eigen-service -p 3000:3000 -v $PWD/data:/app/data  -d ieigen/service:v1",
    "deploy_prod": "docker-compose up -d",
    "doc": "./node_modules/.bin/typedoc --entryPointStrategy expand . --exclude \"node_modules/**\" --exclude \"build/**\"",
    "lint": "./node_modules/.bin/eslint src --ext .ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@eslint/create-config": "^0.1.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.4",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.13",
    "typescript": "^4.4.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.4.0",
    "@ieigen/zkzru": "^0.0.13",
    "asn1.js": "^5.4.1",
    "axios": "^0.23.0",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.4",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "elliptic": "^6.5.4",
    "ethers": "^5.4.7",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "ffjavascript": "^0.2.55",
    "googleapis": "^88.2.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "pubsub-js": "^1.9.4",
    "relay_sdk": "0.0.10",
    "secrets.js-grempe": "^2.0.0",
    "sequelize": "^6.7.0",
    "set-interval-async": "^2.0.3",
    "socket.io": "^4.4.1",
    "sqlite3": "^5.0.2",
    "standard-ecies": "^2.0.0",
    "totp.js": "^0.0.1",
    "typings": "^0.6.10"
  }
}
